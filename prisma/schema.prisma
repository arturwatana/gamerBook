// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model Game {
  id String @id
  name String
  players Int
}

model Player {
  id String @id 
  name String
  age Int
  email String @unique
  games Game
}

